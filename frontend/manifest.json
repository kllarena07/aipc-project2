{
    "manifest_version": 3,
    "name": "Intel Browser Extension",
    "version": "1.0",
    "description": "Project for Intel",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch?v=*"],
            "js": ["content_script.js"]
        }
    ]
}